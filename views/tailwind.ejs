<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Tailwind Typography Reference</title>
</head>
<body class="hidden">
    <!-- Headings -->
    <h1 class="text-4xl text-5xl font-bold font-extrabold text-gray-900 dark:text-white mb-4 leading-tight tracking-tight">H1 Heading</h1>
    <h2 class="text-3xl text-4xl font-semibold font-bold text-gray-800 dark:text-gray-200 mb-3 leading-tight">H2 Heading</h2>
    <h3 class="text-2xl text-3xl font-semibold text-gray-700 dark:text-gray-300 mb-2">H3 Heading</h3>
    <h4 class="text-xl font-medium text-gray-600 dark:text-gray-400 mb-2">H4 Heading</h4>
    <h5 class="text-lg font-medium text-gray-500 dark:text-gray-500 mb-1">H5 Heading</h5>
    <h6 class="text-base font-medium text-gray-400 dark:text-gray-600 mb-1">H6 Heading</h6>

    <!-- Paragraph Styles -->
    <p class="text-base text-lg leading-relaxed text-gray-600 dark:text-gray-300 mb-4">Paragraph with base styling</p>

    <!-- Unordered Lists -->
    <ul class="list-disc list-inside space-y-2 text-gray-700 dark:text-gray-300">
        <li>Disc list inside</li>
        <li>Another disc list item</li>
    </ul>

    <ul class="list-disc list-outside pl-5 space-y-2 text-gray-700 dark:text-gray-300">
        <li>Disc list outside</li>
        <li>Another disc list item</li>
    </ul>

    <!-- Ordered Lists -->
    <ol class="list-decimal list-inside space-y-2 text-gray-700 dark:text-gray-300">
        <li>Decimal list inside</li>
        <li>Another decimal list item</li>
    </ol>

    <ol class="list-decimal list-outside pl-5 space-y-2 text-gray-700 dark:text-gray-300">
        <li>Decimal list outside</li>
        <li>Another decimal list item</li>
    </ol>

    <!-- Additional List Types -->
    <ul class="list-none space-y-2 text-gray-700 dark:text-gray-300">
        <li>No list marker</li>
        <li>Another item without marker</li>
    </ul>

    <ul class="list-square list-inside space-y-2 text-gray-700 dark:text-gray-300">
        <li>Square list marker</li>
        <li>Another square list item</li>
    </ul>

    <!-- Text Alignment -->
    <div class="text-left text-center text-right text-justify"></div>

    <!-- Font Weights -->
    <div class="font-thin font-extralight font-light font-normal font-medium font-semibold font-bold font-extrabold font-black"></div>

    <!-- Text Sizes -->
    <div class="text-xs text-sm text-base text-lg text-xl text-2xl text-3xl text-4xl text-5xl text-6xl text-7xl text-8xl text-9xl"></div>
    <img src="" alt="" class="rounded-lg max-w-full h-auto">
</body>
</html>